{%- liquid
  assign rtl = false
  assign rtl_list = 'ae,ar,arc,bcc,bqi,ckb,dv,fa,glk,ha,he,kwh,ks,ku,mzn,nqo,pnb,ps,sd,ug,ur,yi'
  assign iso_code = localization.language.iso_code | split: '-' | first
  if rtl_list contains iso_code
    assign rtl = true
  endif
  assign product_variants = false
  assign full_width = section.settings.full_width
  if section.settings.background_img != blank
    capture alt
      if section.settings.background_img.alt != blank
        echo section.settings.background_img.alt
      else
        echo product.title
      endif
    endcapture
  endif
  assign background_color = false
  if section.settings.background != 'rgba(0,0,0,0)' and section.settings.background != blank
    assign background_color = true
    assign background_color_section = section.settings.background
  endif
  if section.settings.background_gradient != 'rgba(0,0,0,0)' and section.settings.background_gradient != blank
    assign background_color = true
    assign background_color_section = section.settings.background_gradient
  endif
  capture classes
    echo 'dynamic-section dynamic-section-'
    echo section.id
    echo ' section-padding-top section-padding-bottom section-margin-bottom '
    if section.settings.padding_border
      echo 'section-border '
    endif
  endcapture
  assign overlay_color = false
  if section.settings.background_img_overlay != 'rgba(0,0,0,0)' and section.settings.background_img_overlay != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay
  endif
  if section.settings.background_img_overlay_gradient != 'rgba(0,0,0,0)' and section.settings.background_img_overlay_gradient != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay_gradient
  endif
  assign banner = false
  if section.settings.enable_lazy
    assign lazy = 'lazy'
  else
    assign lazy = 'eager'
  endif
  assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media
-%}
{% style %}
  .dynamic-section-{{ section.id }} {
    --margin_padding: {{ section.settings.padding }}vh;
    {%- if background_color -%}
    --background:{{ background_color_section }};
    --button_primary_text:{{ section.settings.background }};
    --button_secondary_hover:{{ section.settings.background }};
    {%- endif -%}
    --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
    --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
    --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;
  }
  .dynamic-section-{{ section.id }} .banner-section {
    {%- if overlay_color -%}
    --overlay_background: {{ overlay_color_section }};
    --overlay_opacity: {{ section.settings.background_img_overlay_opacity | times: 0.01 }};
    {%- endif -%}
  }
{% endstyle %}
<script defer>
  const productjson = {{ product | json }}
</script>
<section
  id="product-id-{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product_page"
  data-product-type="page"
  data-href="{{ product.url | within: collection }}"
  data-url="{{ 'script.product.js' | asset_url }}"
  data-atc="{% if product.tags contains settings.preorder %}{{ settings.preorder }}{% else %}{{ 'products.product.add_to_cart' | t }}{% endif %}"
  data-cart="{{ section.settings.go_to_cart_enable }}"
  data-zoom="{% unless rtl %}{{ section.settings.zoom_enable }}{% else %}false{% endunless %}"
  data-mobile-zoom="{{ section.settings.zoom_mobile_enable }}"
  data-image="{{ product.featured_image | image_url: width: 260 }}"
  data-image-aspect="{% if product.featured_image.aspect_ratio > 1 %}landscape{% elsif product.featured_image.aspect_ratio < 1 %}vertical{% else %}square{% endif %}"
  data-history="true"
  class="outerProductCont product-id product-{{ section.id }} product-template {{ classes }}{% if settings.animation_enable %}animate-section-div {% endif %}clearfix"
>
  {% if overlay_color or section.settings.background_img != blank %}
    <div class="banner-section {% if full_width %}bottompad-half {% if section.settings.background_img != blank or overlay_color %}relative {% if section.settings.background_img != blank %}lazycontainer lazyignore {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
      {%- liquid
        assign banner = true
        if full_width and section.settings.background_img != blank
          render 'image.load', img_src: section.settings.background_img, size: section.settings.background_img.width, alt: alt, background: true, full_width: true, lazy: lazy, sizes: '100vw'
        endif
      -%}
      <div class="relative container{% if full_width %} half-pad{% else %} {% if section.settings.background_img != blank or overlay_color %}inner-container{% else %}half-pad{% endif %}{% endif %}{% if settings.animation_enable %} animate-section load{% endif %} clearfix">
        <div class="{% if full_width == false %}{% if section.settings.background_img != blank or overlay_color %}inner-container-pad relative {% if section.settings.background_img != blank %}lazycontainer lazyignore {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
          {%- liquid
            if full_width == false and section.settings.background_img != blank
              render 'image.load', img_src: section.settings.background_img, size: section.settings.background_img.width, alt: alt, background: true, lazy: lazy, sizes: '(min-width: 1220px) 1180px, calc(100vw - 40px)'
            endif
          -%}
          <div class="relative table content-height">
            <div class="table-cell {{ section.settings.vertical_content_alignment }}">
              <div
                class="twelve animate {{ section.settings.text_alignment }} {{ section.settings.horizontal_content_alignment }} bottompad-quarter section-height clearfix"
              ></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  <div class="innerProductCont container {% if banner %}inner-container {% else %}half-pad {% endif %}{% if settings.animation_enable %}animate-section animate-section-margin load {% endif %}clearfix">
    <div class="product-grid-product-template product-grid-{{ section.id }}{% if banner %}{% unless full_width %} inner-container-grid{% endunless %}{% endif %}">
      <div class="product-cont {% if banner %}background-white {% endif %}">
        {% if product.media.size > 0 %}
          <div class="product-images six left">
            {%- render 'product.imagesCustom',
              rtl: rtl,
              section_id: section.id,
              lazy: lazy,
              sizes_main: '580,360,,60',
              sizes_thumb: '130,125'
            -%}
          </div>
        {% endif %}

        <div
          id="product-details-{{ section.id }}"
          class="product-details {% if product.media.size > 0 %}five right{% else %}eight center{% endif %}{% unless product.media.size > 0 %}{% endunless %} {% if section.settings.centered %}text-center{% else %}text-left{% endif %} bottompad-quarter"
        >
          {%- liquid
            capture form_id
              echo 'add-item-form-'
              echo section.id
            endcapture
            capture form_class
              if section.settings.go_to_cart_enable
                echo 'go-to-cart-'
                echo section.id
                echo ' '
              endif
              echo 'product-form-'
              echo section.id
              echo ' variants form-validation clearfix'
            endcapture
            assign variant = product.selected_or_first_available_variant
          -%}

            {% form 'product', product, id: form_id, class: form_class, data-product-form: '' %}
              <div class="tab-body{% if section.settings.centered %} center clearfix{% endif %}">
                {%- liquid
                  for block in section.blocks
                    case block.type
                      when 'papertrail'
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        else
                          echo 'bottompad-quarter '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.breadcrumbs'
                        echo '</div>'
                      when 'customtitle'
                        render 'CustomProductTitle'
                      when 'variantselector'
                        render 'CustomVariantSelector'
                      when 'defaultvariantselector'
                        render 'cdefaultvariantselector'
                      when 'socialshare'
                        render 'social.share', share_title: product.title, share_permalink: product.url, share_image: product.featured_media, alignment: 'text-left'
                      when 'customsocialshare'
                        render 'customsocial.share', share_title: product.title, share_permalink: product.url, share_image: product.featured_media, alignment: 'text-left', show_desktop: block.settings.show_desktop, show_mobile: block.settings.show_mobile
                      when 'productaddon'
                        render 'product.addon'
                      when 'productaccordion'
                        render 'product.accordion', more_information: block.settings.more_information, shipping: block.settings.shipping.content
                      when 'customquantity'
                        render 'product.cusotmquantity'
                      when 'custombuybutton'
                        render 'product.custombuybutton', product: product
                      when 'bestoffer'
                        render 'product-offer', product: product
                      when 'bulkbuy'
                        render 'bulkbuy', checkbox: block.settings.bulkbuy_checkbox, text: block.settings.bulkbuy_text
                      when 'title'
                        if block.settings.title_h1
                          assign title = 1
                        else
                          assign title = 2
                        endif
                        echo '<div class="product-header '
                        if block.settings.divider
                          echo 'form-blocks meta '
                        endif
                        echo 'clearfix"><h'
                        echo title
                        echo ' class="section-title-'
                        echo block.settings.title_size
                        echo '">'
                        echo product.title
                        echo '</h'
                        echo title
                        echo '></div>'

                      when 'skuvendor'
                        render 'product.SkuVendor', product: product, block: block, vendor_enable: block.settings.vendor_enable, sku_enable: block.settings.sku_enable, bar_enable: block.settings.bar_enable, divider: block.settings.divider

                      when 'rating'
                        render 'product.rating', product: product, block: block, product_page: true, divider: block.settings.divider

                      when 'price'
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.price', price_size: block.settings.price_size, price_msg: block.settings.price_msg
                        echo '</div>'

                      when 'customprice'
                        render 'product.customprice'

                      when 'payments'
                        render 'product.payments', form: form, block: block, divider: block.settings.divider

                      when 'availability'
                        render 'product.availability', variant: variant, block: block, product_availability_total: block.settings.product_availability_total, divider: block.settings.divider, banner: false

                      when 'properties'
                        capture all_properties
                          render 'product.properties', block: block, text: block.settings.text, text_label: block.settings.text_label, select_title: block.settings.select_title, select_body: block.settings.select_body, select_label: block.settings.select_label, checkbox: block.settings.checkbox, radio_body: block.settings.radio_body, radio_label: block.settings.radio_label, divider: block.settings.divider
                        endcapture
                        if block.settings.tag != blank
                          if product.tags contains block.settings.tag
                            echo all_properties
                          endif
                        else
                          echo all_properties
                        endif

                      when 'variants'
                        assign product_variants = true
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.variants', variant: variant, variant_enable: block.settings.variant_enable, variant_color_enable: block.settings.variant_color_enable, var_length_enable: block.settings.var_length_enable, lazy: lazy
                        echo '</div>'

                      when 'quantity'
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'product.quantity', variant: variant
                        echo '</div>'

                      when 'form'
                        echo '<div class="'
                        if block.settings.divider
                          echo 'form-blocks no-pad '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        if block.settings.gift_enable and product.gift_card?
                          render 'product.gift', product: product, block: block, form: form
                        endif
                        render 'product.buttons', form: form, variant: variant, block: block, smart_enable: block.settings.smart_enable, banner: false
                        echo '</div>'

                      when 'shipping'
                        echo '<div class="shipping_msg bottompad-quarter '
                        if block.settings.divider
                          echo 'form-blocks '
                        endif
                        echo 'clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render 'shipping', size: 26
                        echo '</div>'

                      when 'store'
                        render 'product.storeAvailability', block: block

                      when '@app'
                        echo '<div class="bottompad-quarter clearfix"'
                        echo block.shopify_attributes
                        echo '>'
                        render block
                        echo '</div>'

                      when 'description'
                        capture desc
                          echo product.description | split: '<h6>' | first
                        endcapture
                        if desc != blank
                          echo '<div class="'
                          if block.settings.divider
                            echo 'form-blocks meta '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '>'
                          echo desc
                          echo '</div>'
                        endif

                      when 'button'
                        if block.settings.link != blank and block.settings.text != blank
                          echo '<div class="'
                          if block.settings.divider
                            echo 'form-blocks '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '>'
                          echo '<a href="'
                          echo block.settings.link
                          echo '" class="btn'
                          if block.settings.outline_button
                            echo ' outline'
                          endif
                          unless block.settings.divider
                            echo ' bottompad-half'
                          endunless
                          echo '" title="'
                          echo block.settings.text
                          echo '">'
                          echo block.settings.text
                          echo '</a>'
                          echo '</div>'
                        endif

                      when 'meta'
                        assign meta_size = block.settings.meta | strip_html
                        capture all_meta
                          if block.settings.meta != blank and meta_size.size > 0
                            echo '<div class="'
                            if block.settings.divider
                              echo 'form-blocks meta '
                            endif
                            echo 'clearfix"'
                            echo block.shopify_attributes
                            echo '>'
                            echo block.settings.meta
                            echo '</div>'
                          endif
                        endcapture
                        if block.settings.tag != blank
                          if product.tags contains block.settings.tag
                            echo all_meta
                          endif
                        else
                          echo all_meta
                        endif

                      when 'liquid'
                        assign liquid_size = block.settings.liquid | strip_html
                        capture all_liquid
                          echo '<div id="liquid-'
                          echo block.id
                          echo '" class="'
                          if block.settings.enable_metafields
                            echo 'variant_metafields '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '>'
                          if block.settings.liquid != blank and liquid_size.size > 0
                            echo '<div class="'
                            if block.settings.divider
                              echo 'form-blocks meta'
                            else
                              echo 'bottompad-quarter'
                            endif
                            echo '">'
                            echo block.settings.liquid
                            echo '</div>'
                          endif
                          echo '</div>'
                        endcapture
                        if block.settings.tag != blank
                          if product.tags contains block.settings.tag
                            echo all_liquid
                          endif
                        else
                          echo all_liquid
                        endif

                      when 'image'
                        capture all_imgs
                          echo '<div class="'
                          if block.settings.divider
                            echo 'form-blocks '
                          else
                            echo 'bottompad-quarter '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '><div class="relative lazycontainer table icon-text-table'
                          if section.settings.centered
                            echo ' center'
                          endif
                          echo '">'
                          capture alt
                            if block.settings.block_img.alt != blank
                              echo block.settings.block_img.alt
                            elsif block.settings.caption != blank
                              echo block.settings.caption
                            else
                              echo block.settings.block_img | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
                            endif
                          endcapture
                          if block.settings.block_img != blank
                            render 'image.load', img_src: block.settings.block_img, size: 560, alt: alt, lazy: lazy, sizes: '(min-width: 600px) 560px, (min-width: 768px) 480px, calc(100vw - 40px)'
                          else
                            echo 'image' | placeholder_svg_tag: 'placeholder-svg'
                          endif
                          echo '</div></div>'
                        endcapture
                        if block.settings.tag != blank
                          if product.tags contains block.settings.tag
                            echo all_imgs
                          endif
                        else
                          echo all_imgs
                        endif

                      when 'icon'
                        capture all_icons
                          echo '<div class="'
                          if block.settings.divider
                            echo 'form-blocks '
                          else
                            echo 'bottompad-quarter '
                          endif
                          echo 'clearfix"'
                          echo block.shopify_attributes
                          echo '><div class="table icon-text-table'
                          if section.settings.centered
                            echo ' center'
                          endif
                          echo '">'
                          for i in (1..3)
                            assign enable_number = 'enable_' | append: i
                            assign enable = block.settings[enable_number]

                            assign icon_number = 'icon_' | append: i
                            assign icon = block.settings[icon_number]

                            assign block_img_number = 'block_img_' | append: i
                            assign block_img = block.settings[block_img_number]

                            assign icon_color_number = 'icon_color_' | append: i
                            assign icon_color = block.settings[icon_color_number]

                            assign icon_color_h_number = 'icon_color_h_' | append: i
                            assign icon_color_h = block.settings[icon_color_h_number]

                            assign text_number = 'text_' | append: i
                            assign text = block.settings[text_number]

                            assign pop_up_number = 'pop_up_' | append: i
                            assign pop_up = block.settings[pop_up_number]

                            assign page_number = 'page_' | append: i
                            assign page = block.settings[page_number]

                            render 'product.icons', enable_number: enable_number, enable: enable, icon: icon, block_img: block_img, icon_color: icon_color, icon_color_h: icon_color_h, text: text, pop_up: pop_up, page: page
                          endfor
                          echo '</div></div>'
                        endcapture

                        if block.settings.tag != blank
                          if product.tags contains block.settings.tag
                            echo all_icons
                          endif
                        else
                          echo all_icons
                        endif

                      when 'divider'
                        echo '<div class="form-blocks no-pad clearfix"'
                        echo block.shopify_attributes
                        echo '></div>'
                    endcase
                  endfor

                  if section.settings.include_social
                    if section.settings.centered
                      assign alignment = 'text-center'
                    else
                      assign alignment = 'text-left'
                    endif
                    render 'social.share', share_title: product.title, share_permalink: product.url, share_image: product.featured_media, alignment: alignment
                  endif

                  unless product_variants
                    echo '<div class="hidden">'
                    render 'product.variants', variant: variant
                    echo '</div>'
                  endunless
                -%}
              </div>
              <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
              {% if product.id != '39651226812525' %}
                <input type="hidden" name="properties[_id]" value="">
                <input type="hidden" name="properties[giftWrap]" value="">
                <input type="hidden" name="properties[_giftWrap]" value="">
              {% endif %}
              {%- render 'product.json' -%}
            {% endform %}

        </div>
      </div>
    </div>
  </div>
</section>



{%- liquid
  assign current_variant = product.selected_or_first_available_variant
  assign product_url = product.url | within: collection
-%}

<script>
        document.getElementById('add-item-form-{{ section.id }}').addEventListener('submit', function(event) {
            // Prevent the default form submission behavior
            event.preventDefault();
            console.log('form delayed')

            // Get form data (you can customize this based on your form fields)
            const formData = new FormData(event.target);
            let premurl = location.origin; // permanent url of the store
            let urlparams = []
            // Log the FormData to the console
            formData.forEach((value, key) => {
                console.log(`${key}: ${value}`);
                if(value) urlparams.push(`${key}=${value}`)
                else urlparams.push(`${key}`)
            });
            // adding the section in the request
            urlparams.push(`sections=cart-drawer`)
            
            let finalurl = premurl + `/cart/add.js?` + urlparams.join(`&`)
            console.log(`finalurl `,finalurl)
            $('html').addClass('js-ajax-cart-processing')
            toastatcclick();
            theme.openCart()
            // Make the AJAX request
            fetch(finalurl, {
                method: 'GET' // or 'POST' if required by the API
            })
            .then(response => response.text())
            .then(data => {

                let jsonres = JSON.parse(data);

                let formhtml = new DOMParser()
                  .parseFromString(jsonres.sections['cart-drawer'],'text/html')
                  .querySelector('#cart_drawer').innerHTML;
                console.log('formhtml-', formhtml);
                let cartblock = document.querySelector('#cart_drawer');
                cartblock.innerHTML = formhtml;
                // cartblock.innerHTML = ''
                let newcartitem = parseInt($(`span[data-ajax-cart-bind="item_count"]`).text()) + parseInt($('#quantity').val());
               
                $(`span[data-ajax-cart-bind="item_count"]`).text(newcartitem);
                
                console.log('Response:', jsonres);
                
                 $('html').removeClass('js-ajax-cart-empty').addClass('js-ajax-cart-not-empty');

   
                $('html').removeClass('js-ajax-cart-processing')
                // setTimeout(theme.openCart(), 200)
                
               //checking and adding products for product addon
                pdpAddon();
                atcty();
                if (
                  localStorage.getItem("discountCode") != null &&
                  localStorage.getItem("discountCode") != undefined &&
                  localStorage.getItem("discountCode").trim() != ""
                ) {
                  let storedCoupon = localStorage.getItem("discountCode");
                  theme.applyCodes(storedCoupon);
                } else {
                  console.log("Coupon code is not present on cart open");
                }
            })
            .catch(error => {
                // Handle errors here
                $('html').removeClass('js-ajax-cart-processing')
                console.error('Error:', error);
            });
          
        });
    </script>

<script type="application/ld+json">
  {
   "@context": "http://schema.org",
   "@graph": [
      {
        "@type": "BreadcrumbList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "name": {{ 'general.breadcrumbs.home' | t | json }},
            "item": "{{ shop.url }}"
          },
          {%- if collection -%}
          {
            "@type": "ListItem",
            "position": 2,
            "name": {{ collection.title | json }},
            "item": {{ shop.url | append: collection.url | json }}
          }, {
            "@type": "ListItem",
            "position": 3,
            "name": {{ product.title | json }},
            "item": {{ shop.url | append: product.url | json }}
          }
          {%- else -%}
          {
            "@type": "ListItem",
            "position": 2,
            "name": {{ product.title | json }},
            "item": {{ shop.url | append: product.url | json }}
          }
          {%- endif -%}
        ]
      },
      {
        "@type": "Product",
        "name": {{ product.title | json }},
        "productID": {{ product.id | json }},
        "@id": {{ shop.url | append: product_url | json }},
        "url": {{ shop.url | append: product_url | json }},
        {% if product.featured_media %}"image": [{{ product.featured_media.src | image_url: width: product.featured_media.width | prepend: 'https:' | json }}],{% endif %}
        "description": {{ product.description | strip_html | json }},
        {% if current_variant.barcode != blank %}
        "gtin": {{ current_variant.barcode | json }},
        "mpn": {{ current_variant.barcode | json }},
        {% endif %}
        {% if current_variant.sku != blank %}"sku": {{ current_variant.sku | json }},{% endif %}
        "brand": {
          "@type": "Thing",
          "name": {{ product.vendor | json }}
        },
        "offers": [
        {%- for variant in product.variants -%}
        {%- assign variant_url = variant.url | within: collection -%}
        {
          "@type": "Offer",
          {% if variant.barcode != blank %}
          "gtin": {{ variant.barcode | json }},
          "mpn": {{ variant.barcode | json }},
          {% endif %}
          {% if variant.sku != blank %}"sku": {{ variant.sku | json }},{% endif %}
          "priceCurrency": {{ cart.currency.iso_code | json }},
          "price": "{{ variant.price | divided_by: 100.00 }}",
          "priceValidUntil": "{{ 'now' | date: '%s' | plus: 31536000 | date: '%Y-%m-%d' | uri_encode | replace:'+','%20' }}",
          "availability": "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
          "url" : {{ shop.url | append: variant_url | json }},
          "seller": {
            "@type": "Organization",
