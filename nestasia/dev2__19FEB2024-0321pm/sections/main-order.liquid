                                {% if line_item.final_price > 0 %}
                                  {{ line_item.final_price | money }}
                                {% else %}
                                  {{ 'cart.general.free' | t }}
                                {% endif %}
                                <del>
                                  {% if line_item.original_price > 0 %}
                                    {{ line_item.original_price | money }}
                                  {% else %}
                                    {{ 'cart.general.free' | t }}
                                  {% endif %}
                                </del>
                              {% else %}
                                {{ line_item.original_price | money }}
                              {% endif %}
