  }
  .itm-info {
    font-size: 16px;
    color: #000;
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  .itm-info .sku {
    line-height: 22px;
  }
  .smQtyorderpage {
    text-wrap: nowrap;
  }
  .account-page-main-wrapper .order-details-page .content-wrapper .account-user h3 {
    margin: 0;
    font-size: 18px;
    font-weight: 700;
    font-family: var(--body_font);
    line-height: 35px;
  }
  .account-page-main-wrapper .order-details-page .content-wrapper .account-user p {
    margin: 0;
    line-height: 28px;
    font-size: 14px;
  }
  .shippingandbill_mobileSm {
    display: none;
  }
  .account-page-main-wrapper .order-details-page .footer-item-wrap {
    padding: 5px 0;
    display: flex;
  }
  .UnderSmAccountTableTotalOrder {
    margin-top: 20px;
    color: #000;
    margin-bottom: 70px;
  }
  .account-page-main-wrapper .order-details-page .order-number,
  .account-page-main-wrapper .order-details-page .order-date {
    font-weight: 700;
    margin: 0;
  }
  .Order_Payment_Summary {
    display: none;
  }
  .trackingrow {
    display: flex;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 20px;
  }
  .order-tracking {
    text-align: left;
    width: 24.33%;
    position: relative;
    display: block;
  }
  .order-tracking .is-complete {
    display: block;
    position: relative;
    border-radius: 50%;
    height: 20px;
    width: 20px;
    border: 0 solid #afafaf;
    background-color: #d9d9d9;
    transition: background 0.25s linear;
    -webkit-transition: background 0.25s linear;
    z-index: 2;
  }
  .order-tracking.completed .is-complete {
    border-color: #f4958c;
    border-width: 0;
    background-color: #f4958c;
  }
  .order-tracking.completed p {
    font-size: 12px;
    line-height: 19px;
    text-align: left;
    text-transform: capitalize;
    color: #f4958c;
  }
  .order-tracking p {
    color: #a4a4a4;
    font-size: 12px;
    margin-top: 8px;
    margin-bottom: 0;
    line-height: 19px;
  }
  .order-tracking.completed .is-complete:after {
    border-color: #fff;
    border-width: 0 2px 2px 0;
    width: 4px;
    left: 7px;
    opacity: 1;
  }
  .order-tracking p span {
    font-size: 12px;
    color: #171717;
    font-weight: 300;
  }
  .order-tracking:not(:first-child)::before {
    content: '';
    display: block;
    height: 3px;
    width: calc(100% - 20px);
    background-color: #d9d9d9;
    top: 10px;
    position: absolute;
    right: calc(100% + 2px);
    z-index: 0;
    border-radius: 5px;
  }
  .order-tracking.completed:before {
    background-color: #f4958c;
  }
  .order-tracking .is-complete:after {
    display: block;
    position: absolute;
    content: '';
    height: 10px;
    width: 7px;
    top: -2px;
    bottom: 0;
    left: 5px;
    margin: auto 0;
    border: 0 solid #afafaf;
    border-width: 0 2px 2px 0;
    transform: rotate(45deg);
    opacity: 0;
  }
  .OrderHelp h2 {
    font-family: 'glacialIndifference-bold';
    font-size: 16px;
    line-height: 19px;
    text-transform: capitalize;
    margin-bottom: 0;
    font-family: var(--body_font);
    margin-top: 20px;
    color: #171717;
  }
  @media screen and (max-width: 800px) {
    .trackingrow {
      flex-direction: column;
      padding: 5px 20px;
    }
    .order-tracking {
      margin-bottom: 70px;
      width: 100%;
      display: flex;
    }
    .order-tracking:last-child {
      margin-bottom: 0;
    }
    .order-tracking.completed p {
      font-size: 12px;
    }
    .order-tracking p {
      font-size: 12px;
      margin-top: 0;
      line-height: 20px;
      display: flex;
      width: calc(100% - 30px);
      justify-content: space-between;
      margin-left: 10px;
    }
    .order-tracking.completed .is-complete:after {
      width: 3px;
    }
    .order-tracking p span {
      font-size: 10px;
    }
    .order-tracking:not(:first-child)::before {
      height: 80px;
      width: 2px;
      top: -83px;
      left: 9px;
      right: auto;
    }
    .account-page-main-wrapper .order-details-page #order-cancelled .cancel-label,
    .account-page-main-wrapper .order-details-page #order-cancelled .cancel-note {
      font-size: 12px;
      padding-bottom: 3px;
    }
    .status {
      padding-bottom: 8px;
    }
    .track-order-flex {
      padding: 7px 10px;
      background: #f9ece5;
      margin: 0;
    }
    h2.track-order-ship-title {
      font-size: 16px;
    }
    .account-page-main-wrapper .content.customers-page-wrapper {
      padding: 15px 0 !important;
      background-color: white !important;
    }
    .Order_Payment_Summary {
      display: block !important;
      background: #f1cfbf66;
      font-size: 14px;
      line-height: 17px;
      color: #2d2d2d;
      padding: 9px;
      font-weight: 700;
      margin-bottom: 0;
    }
    .Order_status ul li {
      font-size: 12px;
      line-height: 19px;
      padding: 0;
    }
    .account-page-main-wrapper .order-details-page .order-date {
      font-size: 12px;
      margin: 0;
      margin-top: 10px;
    }
    .account-page-main-wrapper .account-sidebar-wrap,
    .account-page-main-wrapper .main-content-wrapper {
      width: 100%;
      padding: 0;
    }
    .itm-info {
      display: block;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-details .product-price {
      font-size: 14px;
      line-height: 17px;
      letter-spacing: 0.01em;
      text-transform: capitalize;
    }
    .account-page-main-wrapper .order-details-page .order-date {
      font-size: 12px;
      margin: 0;
      margin-top: 10px;
      font-weight: 400;
      line-height: calc(var(--large) * 1.25);
    }
    .account-page-main-wrapper .order-details-page .order-number {
      font-size: 14px;
      line-height: calc(var(--large) * 1.25);
    }
    .account-page-main-wrapper .content.customers-page-wrapper .dynamic-section {
      background-color: white;
    }
    .trackBar {
      background: rgba(241, 207, 191, 0.4);
    }
    .account-page-main-wrapper .order-details-page .order-header-wrapper .twelve {
      padding: 0 10px !important;
      margin: 0;
    }
    .account-page-main-wrapper .order-details-page .order-number {
      display: flex;
      align-items: center;
      font-weight: 400;
    }
    .order-details-page .eight,
    .order-details-page .four {
      width: 100%;
      padding: 0;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper:last-child {
      padding-bottom: 6px;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper {
      border: 0;
      box-shadow: none;
      padding: 0;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-image {
      border-radius: 0;
      padding: 0;
      border: 0;
      max-width: 100px;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-image img {
      width: 100px;
      border-radius: 0;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-details-wrap {
      margin-left: 10px;
      width: calc(100% - 100px);
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-details .item-title-wrap * {
      overflow: hidden;
      text-overflow: ellipsis;
      height: 18px;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-details .item-title-wrap * {
      color: #1e1e1e;
      font-size: 14px;
      line-height: 16px;
    }
    .account-page-main-wrapper .order-details-page .item-wrapper .item-thumb-wrap {
      width: 100px;
      padding: 0;
    }
    .itm-info {
      font-size: 12px;
      line-height: 17px;
      text-transform: capitalize;
      color: #1e1e1e;
      padding: 0;
    }
    .itm-info .sku {
      line-height: 22px;
    }
    .UnderSmAccountTableTotalOrder {
      margin-top: 15px;
      margin-bottom: -15px;
    }
    .account-page-main-wrapper .order-details-page .content-wrapper .account-user h3 {
      background: #f1cfbf66;
      padding: 9px;
      font-size: 14px;
    }
    .order-details-page .account-table {
      background: #fff;
      margin-bottom: 15px;
    }
    .account-page-main-wrapper .order-details-page .content-wrapper .account-user p {
      font-size: 14px;
      padding-left: 12px;
      color: #1e1e1e;
      padding-top: 13px;
      line-height: 19px;
      padding-bottom: 0px;
    }
    .account-page-main-wrapper .order-details-page .footer-item-wrap .orders {
      text-align: left;
      color: #1e1e1e;
    }
  }
</style>

{%- liquid
  assign full_width = section.settings.full_width
  if section.settings.background_img != blank
    capture alt
      if section.settings.background_img.alt != blank
        echo section.settings.background_img.alt
      elsif section.settings.title != blank
        echo section.settings.title
      else
        echo 'customer.order.title' | t
      endif
    endcapture
  endif
  assign text_color = false
  if section.settings.section_color != 'rgba(0,0,0,0)' and section.settings.section_color != blank
    assign text_color = true
    assign text_color_section = section.settings.section_color
  endif
  assign background_color = false
  if section.settings.background != 'rgba(0,0,0,0)' and section.settings.background != blank
    assign background_color = true
    assign background_color_section = section.settings.background
  endif
  if section.settings.background_gradient != 'rgba(0,0,0,0)' and section.settings.background_gradient != blank
    assign background_color = true
    assign background_color_section = section.settings.background_gradient
  endif
  capture classes
    echo 'dynamic-section dynamic-section-'
    echo section.id
    echo ' section-padding-top section-padding-bottom section-margin-bottom '
    if section.settings.padding_border
      echo 'section-border '
    endif
  endcapture
  assign overlay_color = false
  if section.settings.background_img_overlay != 'rgba(0,0,0,0)' and section.settings.background_img_overlay != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay
  endif
  if section.settings.background_img_overlay_gradient != 'rgba(0,0,0,0)' and section.settings.background_img_overlay_gradient != blank
    assign overlay_color = true
    assign overlay_color_section = section.settings.background_img_overlay_gradient
  endif
  assign banner = false
  if section.settings.enable_lazy
    assign lazy = 'lazy'
  else
    assign lazy = 'eager'
  endif
-%}

{% style %}
  .dynamic-section-{{ section.id }} {
    --margin_padding: {{ section.settings.padding }}vh;
    {%- if background_color -%}
    --background:{{ background_color_section }};
    --button_primary_text:{{ section.settings.background }};
    --button_secondary_hover:{{ section.settings.background }};
    {%- endif -%}
    --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
    --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
    --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
    --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
    --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
    --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;
  }
  .dynamic-section-{{ section.id }} .banner-section {
    {%- if text_color -%}
    --headline_font_color:{{ text_color_section }};
    {%- endif -%}
    {%- if overlay_color -%}
    --overlay_background: {{ overlay_color_section }};
    --overlay_opacity: {{ section.settings.background_img_overlay_opacity | times: 0.01 }};
    {%- endif -%}
  }
{% endstyle %}

<section
  data-section-id="{{ section.id }}"
  data-section-type="customer"
  class="{{ classes }}{% if settings.animation_enable %}animate-section-div {% endif %}clearfix"
>
  {% comment %}
    {% if section.settings.title_enable
      or section.settings.sub_title != blank
      or section.settings.background_img != blank
    %}
      {%- liquid
        assign block_width = section.settings.block_width
        case block_width
          when '4'
            assign block_width = 'four'
          when '6'
            assign block_width = 'six'
          when '8'
            assign block_width = 'eight'
          when '9'
            assign block_width = 'nine'
        endcase
      -%}
      <div class="banner-section {% if full_width %}bottompad-half {% if section.settings.background_img != blank or overlay_color %}relative {% if section.settings.background_img != blank %}lazycontainer lazyignore {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
        {%- liquid
          if full_width and section.settings.background_img != blank
            assign banner = true
            render 'image.load', img_src: section.settings.background_img, size: section.settings.background_img.width, alt: alt, background: true, full_width: true, lazy: lazy, sizes: '100vw'
          endif
        -%}
        <div class="relative container{% if full_width %} half-pad{% else %} {% if section.settings.background_img != blank or overlay_color %}inner-container{% else %}half-pad{% endif %}{% endif %}{% if settings.animation_enable %} animate-section load{% endif %} clearfix">
          <div class="{% if full_width == false %}{% if section.settings.background_img != blank or overlay_color %}inner-container-pad relative {% if section.settings.background_img != blank %}lazycontainer lazyignore {% endif %}{% if overlay_color %}background-overlay {% endif %}{% endif %}{% endif %}clearfix">
            {%- liquid
              if full_width == false and section.settings.background_img != blank
                assign banner = true
                render 'image.load', img_src: section.settings.background_img, size: section.settings.background_img.width, alt: alt, background: true, lazy: lazy, sizes: '(min-width: 1220px) 1180px, calc(100vw - 40px)'
              endif
            -%}
            <div class="relative table content-height">
              <div class="table-cell {{ section.settings.vertical_content_alignment }}">
                <div class="{{ block_width }} animate {{ section.settings.text_alignment }} {{ section.settings.horizontal_content_alignment }} bottompad-quarter section-height clearfix">
                  {%- liquid
                    if section.settings.title_h1
                      assign title = 1
                    else
                      assign title = 2
                    endif
                  -%}
                  {% if section.settings.title_enable -%}
                    <h{{ title }} class="section-title-{{ section.settings.title_size }}">
                      {%- if section.settings.title != blank -%}
                        {{- section.settings.title | escape -}}
                      {%- else -%}
                        {{- 'customer.order.title' | t }}
                        {{ order.name -}}
                      {%- endif -%}
                    </h{{ title }}>
                    {%- if overlay_color %}{%- assign banner = true -%}{% endif -%}
                  {%- endif %}
                  {% if section.settings.sub_title != blank %}
                    <h3 class="section-title-{{ section.settings.title_sub_size }}">
                      {{ section.settings.sub_title | escape }}
                    </h3>
                    {% if overlay_color %}{%- assign banner = true -%}{% endif -%}
                  {%- endif %}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    {% endif %}
  {% endcomment %}

  {% comment %}
    <div class="container {% if banner %}inner-container {% else %}half-pad {% endif %}{% if settings.animation_enable %}animate-section animate-section-margin load {% endif %}clearfix">
      <div{% if banner %}{% unless full_width %} class="inner-container-grid"{% endunless %}{% endif %}>
        <div class="{% if banner %}background-white {% endif %}clearfix">

          <div class="account-user twelve">

            <div class="return bottompad"><a class="btn outline auto-width" href="{{ routes.account_url }}" title="{{ 'customer.account.return' | t | escape }}">{{ 'customer.account.return' | t }}</a></div>

            <h2 class="order-date">{{ 'customer.order.date' | t }} {{ order.created_at | time_tag: format: 'date_at_time' }}</h2>

            {% if order.cancelled %}
            <div id="order-cancelled" class="errors">
              <p>{{ 'customer.order.cancelled' | t }}</p>
              <p><strong>{{ 'customer.order.cancelled_date' | t }}:</strong> {{ order.created_at | time_tag: format: 'date_at_time' }}<span class="separator" aria-hidden="true"></span><strong>{{ 'customer.order.cancelled_reason' | t }}:</strong> <span class="capitalize">{{ order.cancel_reason_label }}</span></p>
            </div>
            {% endif %}

            <div class="status capitalize bottompad"><strong>{{ 'customer.order.payment_status' | t }}:</strong> <span>{{ order.financial_status_label }}</span><span class="separator" aria-hidden="true"></span><strong>{{ 'customer.order.fulfillment_status' | t }}:</strong> <span>{{ order.fulfillment_status_label }}</span></div>

          </div>

          <div class="six">
            <div class="cart-table account-table">
              <h3>{{ 'customer.order.orders_title' | t }}</h3>
              {% assign images = false %}
              {% for line_item in order.line_items %}
              <div id="{{ line_item.id }}" class="item relative product-id item-row table-center clearfix">
                {%- liquid
                  if line_item.variant.featured_media
                	  assign line_item_featured_media = line_item.variant.featured_media.preview_image
                	elsif line_item.product.featured_media
                	  assign line_item_featured_media = line_item.product.featured_media.preview_image
                	endif
                -%}
                {% if line_item_featured_media %}
                <div class="four negative-left negative-right item-thumb title-align title-align-left">
                  <div class="relative lazycontainer">
                    <a href="{{ line_item.variant.url | within: collections.all }} " title="{{ line_item.title | escape }}" class="keyed">
                      {%- liquid
                        capture alt
                          if line_item_featured_media.alt != blank
                            echo line_item_featured_media.alt
                          else
                            echo line_item.title
                           endif
                        endcapture
                        render 'image.load', img_src: line_item_featured_media, size: 320, alt: alt, nopad: true, lazy: 'eager', sizes: '(min-width: 320px) 230px, (min-width: 768px) 170px, calc(100vw - 100px)'
                      -%}
                    </a>
                  </div>
                </div>
                {% assign images = true %}
                {% endif %}
                <span class="item-details skip half table-center">
                  <div class="{% if line_item_featured_media %}eight{% else %}twelve{% endif %} negative-left negative-right">
                    <h5><a href="{{ line_item.variant.url | within: collections.all }}" title="{{ line_item.title | escape }}">{{ line_item.product.title }}</a>{% if line_item.variant.title and line_item.variant.title != 'Default Title' %}<br/><small>{{ line_item.variant.title }}</small>{% endif %}</h5>
                    {% if line_item.selling_plan_allocation %}
                    <h6 class="selling"><small>{{ line_item.selling_plan_allocation.selling_plan.name }}</small></h6>
                    {% endif %}
                    {%- assign property_size = line_item.properties | size -%}
                    {% if property_size > 0 %}
                    <p class="word-break"><small>
                      {% for p in line_item.properties %}
                      {% assign property_first_char = p.first | slice: 0 %}
                      {% if p.last != blank and property_first_char != '_' %}
                      <strong>{{ p.first }}:</strong> {% if p.last contains '/uploads/' %}<a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>{% else %}{{ p.last }}{% endif %}
                      {% endif %}
                      {% endfor %}
                    </small></p>
                    {% endif %}
                    {% if line_item.sku != blank or line_item.line_level_discount_allocations != blank or line_item.fulfillment%}
