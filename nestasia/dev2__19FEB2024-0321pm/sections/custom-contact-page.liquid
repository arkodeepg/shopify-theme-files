    <div class="contact__page_form_wrapper">
      {% form 'contact' %}
        {% if form.posted_successfully? %}
          <div class="twelve">
            <div class="success successForm feedback">
              <p>{{ 'contact.form.post_success' | t }}</p>
            </div>
          </div>
        {% endif %}
        {% if form.errors %}
          <div class="twelve">
            <div class="errors errorForm feedback">
              <p>
                {{ 'contact.form.post_error' | t }}
                {{ 'contact.form.post_error_msg1' | t }}
                {{ form.errors | size | pluralize: 'error', 'errors' }}
                {{ 'contact.form.post_error_msg2' | t }}
              </p>
              <ul class="unformatted">
                {% for field in form.errors %}
                  {% if field == 'form' %}
                    <li>{{ form.errors.messages[field] }}</li>
                  {% else %}
                    <li>{{ form.errors.translated_fields[field] }} - {{ form.errors.messages[field] }}</li>
                  {% endif %}
                {% endfor %}
              </ul>
            </div>
          </div>
        {% endif %}
        <div id="contactFormWrapper">
          <h2 class="content-form-head">Write to us</h2>
          <span class="form">
            {%- assign name_attr = 'contact.form.name' | t | handle -%}
            <label for="contactFormName">{{ 'contact.form.name' | t }}</label>
            <input
              required
              type="text"
              id="contactFormName"
              name="contact[{{ name_attr }}]"
              placeholder="Full Name"
              autocapitalize="words"
              value="{% if form[name_attr] %}{{ form[name_attr] }}{% elsif customer %}{{ customer.name }}{% endif %}"
              required
            >
          </span>
          <span class="form">
            <label for="contactFormEmail">{{ 'contact.form.email' | t }}</label>
            <input
              required
              type="email"
              id="contactFormEmail"
              name="contact[email]"
              placeholder="Email"
              autocorrect="off"
              autocapitalize="off"
              value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
              required
            >
          </span>
          <!--
            <span class="form">
            {%- assign phone_attr = 'contact.form.phone' | t | handle -%}
            <label for="contactFormTelephone">{{ 'contact.form.phone' | t }}</label>
            <input type="number" id="contactFormTelephone" name="contact[{{ phone_attr }}]" placeholder="{{ 'contact.form.phone_placeholder' | t }}" pattern="[0-9\-]*" value="{% if form[phone_attr] %}{{ form[phone_attr] }}{% elsif customer %}{{ customer.phone }}{% endif %}" class="text-left" />
            </span>
          -->

          <span class="form">
            <label for="contactFormName">{{ 'contact.form.name' | t }}</label>
            <input
              required
              type="text"
              id="contactFormSubject"
              name="contact[subject]"
              placeholder="Subject"
              autocapitalize="words"
              value=""
              required
            >
          </span>
          <span class="form message">
            <label for="contactFormMessage">{{ 'contact.form.message' | t }}</label>
            <textarea
              required
              id="contactFormMessage"
              name="contact[body]"
              placeholder="Message"
              required
            >
              {% if form.body %}
                {{ form.body }}{% endif %}
            </textarea>
          </span>
          <span class="hidden extra" aria-hidden="true">
            <label for="contactFormNumber">Extra</label>
            <input
              type="text"
              id="contactFormNumber"
              name="Extra"
              value=""
            >
          </span>
          <div class="contact-info twelve">
            <span id="honeypot"></span>
            <noscript>
              <span class="btn standard-width" id="contactFormSubmit">Enable Javascript</span>
            </noscript>
          </div>
        </div>
      {% endform %}
    </div>
  </div>
</div>

<style>
  @import url('https://fonts.googleapis.com/css2?family=Lato:ital, wght@0, 300;0, 400;0, 700;0, 900;1, 300;1, 400;1, 700 &display=swap');
  * {
    box-sizing: border-box;
  }

  img {
    vertical-align: middle;
    max-width: 100%;
  }

  .container {
    /*     max-width: 1280px; */
  }

  .page-contact-template .container {
    display: flex;
    /*   flex-wrap: wrap; */
  }

  .page-contact-template .container > div {
    /*     width: 100%;
    height: 100%;
    padding: 80px; */
  }

  .contact__page_form_wrapper {
    height: 100%;
    padding: 80px;
  }

  .contact__page_left_wrapper .section_heading h1 {
    margin: 0 0 24px;
    font-family: var(--headline_font);
    font-style: normal;
    /*     font-weight: 700; */
    font-size: 60px;
    line-height: 71px;
    text-transform: capitalize;
    color: #b39d6f;
  }

  .contact__page_left_wrapper .content p {
    margin: 0 0 40px;
    font-family: 'Lato';
    font-style: normal;
    font-weight: 400;
    font-size: 18px;
    line-height: 32px;
    color: #292d32;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info h4 svg {
    vertical-align: middle;
    max-width: 25px;
    max-height: 25px;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info h4 span {
    vertical-align: middle;
    display: inline-block;
    margin-left: 10px;
    font-family: var(--headline_font);
    font-style: normal;
    font-weight: 400;
    font-size: 20px;
    line-height: 32px;
    text-align: center;
    text-transform: capitalize;
    color: #b39d6f;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info h4 {
    margin: 0 0 5px;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info .text p a {
    text-decoration: none;
    display: inline-block;
    font-family: 'Lato';
    font-style: normal;
    font-weight: 500;
    font-size: 16px;
    line-height: 32px;
    text-align: center;
    text-transform: none;
    color: #171717;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info .text p {
    margin: 0;
    padding: 0;
    font-family: 'Lato';
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    line-height: 32px;
    color: #171717;
  }

  .contact__page_left_wrapper .contact_details_block .contact_info {
    margin-bottom: 40px;
  }
  .contact__page_left_wrapper .contact_details_block .contact_info:last-child {
    margin-bottom: 0;
  }
  .contact__page_left_wrapper .contact_details_block .contact_info h4 svg {
    vertical-align: middle;
    max-width: 25px;
    max-height: 25px;
    color: #b39d6f;
  }

  div#contactFormWrapper > h2.content-form-head {
    margin: 0 0 20px;
    font-family: var(--headline_font);
    font-style: normal;
    font-weight: 700;
    font-size: 24px;
    line-height: 29px;
    color: #171717;
    text-align: center;
    letter-spacing: 0.08em;
    text-transform: uppercase;
  }

  div#contactFormWrapper span.form {
    margin: 0 0 10px;
  }

  #contactFormWrapper span.form label {
