{%- assign full_width = section.settings.full_width -%}

{% if section.settings.background_img != blank %}
  {%- assign img_src = section.settings.background_img -%}
  {%- capture image_data -%}
    {%- render 'image-background-src', img_src: img_src -%}{%- endcapture -%}
  {%- assign img_size = '1x' -%}
{% endif %}

