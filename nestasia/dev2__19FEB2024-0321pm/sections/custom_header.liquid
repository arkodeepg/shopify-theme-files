            class="header-mobile_banner header-mobile_banner-top lazy"
            width="{{ section.settings.mob_banner.width }}"
            height="{{ section.settings.mob_banner.height }}"
            data-src="{{section.settings.mob_banner | image_url: width: '600' }}"
            alt="{{section.settings.mob_banner.alt }}"
          >
        </a>
      {% endif %}

      {%- for link in linklists[section.settings.menu].links -%}
        {% comment %}
          <script defer>
          console.log(`
          ------------------
          {% assign msc = 0 %}
          {% if link.links != blank %}
            {% for sub_link in link.links %}
                {% if sub_link.links != blank %}
                  {% assign msc = msc | plus: 1 %}
                  {{ sub_link.title }}
                {% endif %}
              {% endfor %}
            {% endif %}

              {% assign blk = 0 %}
                {% for sub_link in link.links %}
                    {% if sub_link.links == blank %}
                      {% assign blk = blk | plus: 1 %}}
                      {{ sub_link.title }}
                      {% endif %}{% endfor %}
          {% liquid
            if blk > 0
              assign msc = msc | plus: 1
            endif
          %}

          final msc - {{msc}}
          ------------------

          `)


          </script>
        {% endcomment %}

        {% assign msc = 0 %}
        {% if link.links != blank %}
          {% for sub_link in link.links %}
            {% if sub_link.links != blank %}
              {% assign msc = msc | plus: 1 %}
            {% endif %}
          {% endfor %}
        {% endif %}

        {% assign blk = 0 %}
        {% for sub_link in link.links %}
          {% if sub_link.links == blank %}
            {% assign blk = blk | plus: 1 %}
          {% endif -%}
        {%- endfor %}
        {% liquid
          if blk > 0
            assign msc = msc | plus: 1
          endif
        %}

        <div class="hoverlink1ul linkSM1 flex {{ displayMegamenuCheck }} {% if msc == 1 %}oneline{% endif %}">
          <div class="hoverlink1ul_child {% if link.links != blank %}header_dropdown{% endif %}" title="{{link.title}}">
            <div class="hoverlink1ul_child-title">
              {% if link.title == 'Gifts' -%}
                <a href="{{ link.url }}" class="conlydesktop">{%- render 'svg-icons', icon: 'gift-text' -%}</a>
              {%- endif %}

              <span class="conlymobile">
                {%- if link.title == 'Sale' -%}
                  <img
                    class="lazy"
                    loading="lazy"
                    src="//nestasia.in/cdn/shop/files/1_c2409236-0b6f-4102-bb8b-535ef3d834aa.png?v=1687709585"
                  >
                {%- endif %}
                {% if link.title == 'Luxe' -%}
                  <img
                    class="lazy"
                    loading="lazy"
                    src="//nestasia.in/cdn/shop/files/Revamp_Icons_by_Arijit_square-59_1.png?v=1687553319"
                  >
                {%- endif %}
                {% if link.title == 'Gifts' -%}
