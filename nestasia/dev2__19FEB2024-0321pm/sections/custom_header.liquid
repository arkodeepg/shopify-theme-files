                    class="lazy"
                    loading="lazy"
                    src="//nestasia.in/cdn/shop/files/gift-box-svgrepo-com_1.png?v=1687708417"
                  >
                {%- endif %}
              </span>
              <a
                class="{% if link.title == 'Gifts' %}conlymobile{% endif %} {% if link.title == 'Sale' %}menu-red{% endif %} {% if link.title == 'Gifts' or link.title == 'Luxe' %}menu-gold{% endif %}"
                href="{{ link.url }} "
              >
                {{ link.title }}
              </a>
              <span class="conlymobile">
                {%- if link.object.metafields.custom.menu_img -%}
                  {{- link.object.metafields.custom.menu_img | image_url: height: 12 | image_tag -}}
                {%- endif -%}
              </span>
            </div>
            {% if link.links != blank -%}
              <span class="conlymobile smIcon icon icon-chevron-down"></span>
            {%- endif %}
