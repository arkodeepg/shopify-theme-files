<script defer>
  $(document).ready(function(){
    $('.back-btn').click(function(e){
      e.preventDefault();
      $('.orderTrcingMain').hide();
      $('.summary').show();
      $('.hide').show();
    });
    $('.mymodalbtn').click(function(e){
      let order_id = $(this).attr('data-order-id');
      $(`#myModal_${order_id}`).fadeIn();
    });
    $('.modal .close').click(function(e){
      $(this).closest('.modal').fadeOut();
    });
    $('.review_starts:not(.disabled) svg').click(function(e){
      $(this).addClass('active');
      $(this).prevAll().addClass('active');
      $(this).nextAll().removeClass('active');
    });
    $('.orderRateSmTotal:not(.disabled) svg').click(function(e){
      let dataNumber = parseInt($(this).attr("data-number"));
      console.log(dataNumber);
      console.log($(`.totalRevewSmchsvg${dataNumber}`));
      $(`.totalRevewSmchsvg${dataNumber}`).addClass('active');
      $(`.totalRevewSmchsvg${dataNumber}`).prevAll().addClass('active');
      $(`.totalRevewSmchsvg${dataNumber}`).nextAll().removeClass('active');
    });
    
     $('.totalButtonSub').click(function(e){
       $(".BtnForReviewEach").click();
     });
                                                 
    $(document).on('click','.order_page_pro_review_form .btn',function(){
      let $form = $(this).closest('form');
      let counts = 0;
      let count = 0;
      $form.find('.review_starts svg').each(function(e){
        if($(this).hasClass('active')){
          counts++;
        }
      });
      if(counts==0){
        count++;
        $form.find('.review_starts svg').each(function(e){
          $(this).find('path').css('stroke','red');
        });
        setTimeout(function(){
          $form.find('.review_starts svg').each(function(e){
            $(this).find('path').css('stroke','#96865D');
          });
        },3000);
      }
      let review_title = $form.find('input[name="review"]').val();
      if(review_title == ''){
        count++;
        $form.find('input[name="review"]').css('border-color','red');
        setTimeout(function(){
          $form.find('input[name="review"]').css('border-color','#dedede');
        },3000);
      }
      let review_body = $form.find('textarea').val();
      if(review_body == ''){
        count++;
        $form.find('textarea').css('border-color','red');
        setTimeout(function(){
          $form.find('textarea').css('border-color','#dedede');
        },3000);
      }
      if(count!=0){
        return false;
      }
      let productId = $form.attr('data-product-id');
      var reviewData = {
        name:`{{customer.name}}`,
        email: `{{customer.email}}`,
        rating: parseInt(counts),
        title: review_title,
        body:review_body,
        id:productId,
        url:document.domain,
        shop_domain:Shopify.shop
      };
      $.ajax({
        type: "POST",
        url: "https://judge.me/reviews",
        data: reviewData,
        dataType: "json",
        success: function (data) {
          $form.find('.review_starts').addClass('disabled');
          $form.find('.form_group_body').hide();
          $form.siblings('.thank_you_msg').removeClass('hide');
        },
        error: function (error) {
          console.log('error',error);
        }
      });
    });
  });
  function dateModified (OrderDate){
    let dateFilter = OrderDate.toDateString().split(" ");
    let year = dateFilter[3];
    let day = dateFilter[2];
    var months = {
        "Jan": "January",
        "Fab": "February",
        "Mar": "March",
        "Apr": "April",
        "May": "May",
        "Jun": "June",
        "July": "July",
        "Aug": "August",
        "Sep": "September",
        "Oct": "October",
        "Nov": "November",
        "Dec": "December"
    }
    return `${day} ${months[dateFilter[1]]}, ${year}`;
  }

  $(document).on('click','.order_track_btn',function(e){
    e.preventDefault();
    let orderId = $(this).attr('data-orderid');
    let TrackorderIds = $(this).attr('data-ids');
    TrackorderArr = '';
    if(TrackorderIds.includes(',')){
      TrackorderArr = TrackorderIds.split(',');
    }else{
      TrackorderArr = [TrackorderIds];
    }
    for(var o = 0; o < TrackorderArr.length; o++){
      let awbNo = TrackorderArr[o];
      let groupId = `group-awb-${awbNo}`;
      let itemsdata = {
        waybill_no: awbNo
      };
      $.ajax({
        type: "POST",
        url: "https://support.nestasia.in/api/track-delivery",
        data: itemsdata,
        dataType: "json",
        success: function (data) {
          console.log("data",data)
          for(var i=1; i <= data.response.ShipmentData.length; i++){
            let Count = i - 1;
            var statusDateTime = new Date(data.response.ShipmentData[Count].Shipment.Status.StatusDateTime).toDateString();
            $(`#${groupId} .last_updated_dasta`).html(statusDateTime);
            var shipmentStatus = data.response.ShipmentData[Count].Shipment.Status.Status;
            var outDateTime = new Date(data.response.ShipmentData[Count].Shipment.Status.StatusDateTime);   
            var PickUpDate = new Date(data.response.ShipmentData[Count].Shipment.PickUpDate);            
            var ExpectedDeliveryDate = new Date(data.response.ShipmentData[Count].Shipment.ExpectedDeliveryDate);
            var shipmentInstructions =  data.response.ShipmentData[Count].Shipment.Status.Instructions;
            if(shipmentStatus == 'Delivered' && outDateTime != undefined ){
              $(`#${groupId} .shipment_status`).html(shipmentStatus);
              $(`#${groupId} .order-tracking:eq(2)`).addClass('completed');
              $(`#${groupId} .order-tracking:eq(3)`).addClass('completed');
              $(`#${groupId} .OutDate`).css('display','block');
              $(`#${groupId} .OutDate`).html(dateModified(PickUpDate)); 
              $(`#${groupId} .arriving .ariveDate`).html(dateModified(outDateTime)); 
              $(`#${groupId} .last_updated_dasta`).html(dateModified(outDateTime));
            } else{
              $(`#${groupId} .shipment_status`).html(`${shipmentStatus} (${shipmentInstructions})`);
              $(`#${groupId} .order-tracking:eq(2)`).addClass('completed');
              $(`#${groupId} .order-tracking:eq(3)`).removeClass('completed');
              $(`#${groupId} .outDelivery .text`).html(shipmentStatus);
              $(`#${groupId} .OutDate`).css('display','block');
              $(`#${groupId} .OutDate`).html(`${dateModified(PickUpDate)} (${shipmentInstructions})`); 
              $(`#${groupId} .arriving .ariveDate`).html(dateModified(ExpectedDeliveryDate)); 
            }
          }
        },
        error: function (error) {
          console.log('error',error);
        }
      });
    }
    $(`#order_trackModal_${orderId}`).fadeIn();
  });

  if ($(window).width() < 767) {
  $(document).ready(function() {
  $(".moreless-button").on("click", function() {
    var content = $(this).prev(".ReadMoreFeature");
