    } else {
      content.css("max-height", maxHeight + "px");
      $(this).html(`View More <svg xmlns="http://www.w3.org/2000/svg" width="9" height="6" viewBox="0 0 9 6" fill="none"><path d="M0.5 0.5L4.5 4.5L8.5 0.5" stroke="#6C6F7F"/></svg>`);
    }
  });
});
}
